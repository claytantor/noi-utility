<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.noi.utility.spring">
	<class name="Role" table="role">

		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="version" column="version" type="java.lang.Integer"/>
		<property name="role" column="role" type="java.lang.String"/>		
		<property name="roleGroup" column="role_group" type="java.lang.String"/>		
					
	</class>
</hibernate-mapping>